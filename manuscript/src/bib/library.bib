journal = {Theory and Practice of Logic Programming},
pages = {595--600},
pmid = {20035614},
title = {{Concepts, Techniques, and Models of Computer Programming}},
url = {https://mitpress.mit.edu/books/concepts-techniques-and-models-computer-programming},
volume = {5},
year = {2003}
}
