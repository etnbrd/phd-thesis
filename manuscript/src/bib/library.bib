publisher = {ACM Press},
title = {{Introducing 'Bones': a parallelizing source-to-source compiler based on algorithmic skeletons}},
url = {http://dl.acm.org/citation.cfm?id=2159430.2159431 http://dl.acm.org/citation.cfm?id=2159431},
